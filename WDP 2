#include "pch.h"
#include <iostream>
#include <vector>
using namespace std;

int main()
{	
	vector<int> liczby = { 8,3,1,5,4 };
	for (int i = 0; i < liczby.size(); i++)
	{
		cout << liczby[i] << " ";
	}
}



#include <iostream>
#include <vector>
using namespace std;

void wypisz(vector<int> tab) {
	for (int i = 0; i < tab.size(); i++) {
		cout << tab[i] << ", ";
	}
	cout << endl;
}

int dodaj(int a, int b) {
	int wynik = a + b;
	return wynik;
}

int main() {
	int tmp = 5;
	int wynik = dodaj(tmp, 8);
	cout << wynik << endl;
	
	vector<int> liczby = { 1,4,6,7,9 };
	wypisz(liczby);
	liczby.push_back(10);
	wypisz(liczby);
	return 0;
}
